{
    "configurations": [
        {
            "name": "Launch Program",
            "program": "${workspaceFolder}/server.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        },
        {
            "type": "pwa-chrome",
            "request": "launch",
            "name": "Launch Chrome against localhost",
            "url": "http://localhost:8084",
            "webRoot": "${workspaceFolder}/public/agil client/www"
        },
        {
            "name": "Launch Edge",
            "request": "launch",
            "type": "pwa-msedge",
            "url": "http://localhost:8084",
            "webRoot": "${workspaceFolder}/public/agil client/www"
        },
        {
            "name": "Launch Microsoft Edge and open the Edge DevTools",
            "request": "launch",
            "type": "vscode-edge-devtools.debug",
            "url": "http://localhost:8084",
            "webRoot": "${workspaceFolder}/public/agil client/www"
        }
    ]
}